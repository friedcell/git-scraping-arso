    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Petek, 18.6.2021 ob 18h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-06-18 17:34"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Zve\u010der in pono\u010di bo precej jasno. Najni\u017eje jutranje temperature bodo od 13 do 19 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "V soboto bo prete\u017eno jasno, popoldne bo nastala kak\u0161na nevihta. Najvi\u0161je dnevne temperature bodo od 29 do 33 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Precej jasno bo, le v Alpah bo v popoldanskem \u010dasu mo\u017ena kak\u0161na vro\u010dinska nevihta. Vse bolj vro\u010de bo.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad vzhodno in srednjo Evropo ter nad Alpami je obmo\u010dje visokega zra\u010dnega tlaka. Ob \u0161ibkih jugozahodnih vetrovih doteka k nam dokaj suh in topel zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V soboto Iznad vzhodne sega nad srednjo Evropo \u0161ibko obmo\u010dje visokega zra\u010dnega tlaka. Ob \u0161ibkih vetrovih se nad nami zadr\u017euje precej topel in razmeroma suh zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V nedeljo, ponedeljek in torek bo son\u010dno in vro\u010de. V popoldanskem \u010dasu bo nastala kak\u0161na vro\u010dinska nevihta. Ponekod bo pihal jugozahodnik.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Pono\u010di in jutri bo pove\u010dini jasno. Jutri opoldne bo nastala kak\u0161na nevihta.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
        "tsUpdated": "2021-06-18T15:34:21+00:00", 
