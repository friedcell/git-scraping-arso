    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "\u010cetrtek, 20.1.2022 ob 13h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2022-01-20 12:51"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Popoldne bodo padavine ponehale, od severozahoda se bo zjasnilo. Najvi\u0161je dnevne temperature bodo od 3 do 9 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Jutri bo na Primorskem prete\u017eno jasno s \u0161ibko burjo. Drugod bo sprva delno jasno s spremenljivo obla\u010dnostjo, predvsem na severovzhodu bodo kratkotrajne sne\u017ene plohe. \u010cez dan se bo prehodno zjasnilo. Najni\u017eje jutranje temperature bodo od -6 do -3, v alpskih dolinah do -10, na Gori\u0161kem in ob morju okoli 0, najvi\u0161je dnevne od 0 do 5, na Primorskem do 9 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Popoldne se bo od severozahoda jasnilo, padavine v sosednjih krajih Hrva\u0161ke bodo slabele.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Jutri bo ob Jadranu ter v sosednjih krajih Italije precej jasno. Drugod bo delno jasno s spremenljivo obla\u010dnostjo, zjutraj bodo mo\u017ene posamezne sne\u017ene plohe.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Hladna fronta se pomika \u010dez srednjo Evropo nad severni Balkan proti jugovzhodu. Za njo se nad Alpami krepi obmo\u010dje visokega zra\u010dnega tlaka. Od severa bo za\u010del k nam dotekati hladnej\u0161i in bolj suh zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "Jutri Nad zahodno in srednjo Evropo se krepi obmo\u010dje visokega zra\u010dnega tlaka. S severozahodnimi vetrovi k nam doteka suh in hladnej\u0161i zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V soboto bo na Primorskem delno jasno, drugod zmerno do prete\u017eno obla\u010dno. Na severu in vzhodu bo lahko ob\u010dasno naletaval sneg. V nedeljo bo zmerno do prete\u017eno obla\u010dno. Predvsem na vzhodu bodo ob\u010dasno mo\u017ene rahle sne\u017ene padavine.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Popoldne bodo padavine ponehale, jasnilo se bo. Jutri bo deloma son\u010dno, zjutraj bodo na severovzhodu manj\u0161e sne\u017ene plohe.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
