    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Torek, 16.3.2021 ob 18h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-03-16 17:32"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Pono\u010di se bo zjasnilo, zjutraj bo prete\u017eno jasno. Jutranje temperature bodo od -6 do 0 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "V sredo bo delno jasno s spremenljivo obla\u010dnostjo, popoldne bodo posamezne kratkotrajne plohe. Ponekod bo \u0161e pihal severni veter. Najvi\u0161je dnevne temperature bodo od 6 do 10, na Primorskem do 13 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "V sredo bo delno jasno z ob\u010dasno pove\u010dano obla\u010dnostjo, predvsem v Alpah in v krajih vzhodno od nas bo nastalo nekaj ploh. Pihal bo severni veter.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad vzhodno Evropo je ciklonsko obmo\u010dje, nad zahodno Evropo pa obmo\u010dje visokega zra\u010dnega tlaka. V vi\u0161inah priteka z vetrovi severnih smeri hladen in ob\u010dasno bolj vla\u017een zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V \u010detrtek bo v vzhodni Sloveniji prete\u017eno obla\u010dno, drugod delno jasno s spremenljivo obla\u010dnostjo. Popoldne bodo krajevne plohe. Ponekod bo \u0161e pihal veter severnih smeri. V petek in soboto bo prete\u017eno obla\u010dno, ob\u010dasno bodo manj\u0161e krajevne padavine. V soboto bo pihal severovzhodni veter, na Primorskem burja.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "V sredo bo delno jasno s spremenljivo obla\u010dnostjo, popoldne bodo posamezne kratkotrajne plohe. Pihal bo severni veter.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
