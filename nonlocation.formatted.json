    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Torek, 10.8.2021 ob 18h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-08-10 17:32"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Pono\u010di bo prete\u017eno jasno. Najni\u017eje jutranje temperature bodo od 12 do 17, na Primorskem okoli 20 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "V sredo bo son\u010dno, predvsem sredi dneva in popoldne bodo nastale posamezne nevihte. Zapihal bo severovzhodni veter. Najvi\u0161je dnevne temperature bodo od 26 do 32, v Vipavski dolini do 34 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Pono\u010di bo v vseh sosednjih pokrajinah prete\u017eno jasno.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "V sredo bo dokaj son\u010dno s pove\u010dano verjetnostjo za popoldanske nevihte.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad ve\u010djim delom Evrope vztraja obmo\u010dje visokega zra\u010dnega tlaka. V vi\u0161inah priteka nad na\u0161e kraje z vetrovi zahodnih smeri toplej\u0161i in razmeroma suh zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V sredo Nad ve\u010djim delom Evrope vztraja obmo\u010dje visokega zra\u010dnega tlaka. V vi\u0161inah nad na\u0161e kraje prehodno priteka hladnej\u0161i, v spodnjih zra\u010dnih plasteh pa od vzhoda nekoliko bolj vla\u017een zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V \u010detrtek, petek in soboto bo son\u010dno, verjetnost popoldanskih neviht bo zelo majhna. Vro\u010dina se bo stopnjevala.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Pono\u010di in jutri bo prete\u017eno jasno. Jutri sredi dneva in popoldne bodo posamezne nevihte.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
