    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Torek, 31.8.2021 ob 6h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-08-31 05:59"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Danes bo delno jasno s spremenljivo obla\u010dnostjo, zjutraj ponekod po ni\u017einah megleno. Predvsem popoldne bo \u0161e lahko nastala kak\u0161na ploha ali nevihta. Najvi\u0161je dnevne temperature bodo od 20 do 23, na Primorskem do 25 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Jutri bo delno jasno z ob\u010dasno zmerno obla\u010dnostjo, ki jo bo ve\u010d v vzhodnih krajih. Predvsem tam bo pihal severni veter, ponekod na Primorskem pa prehodno \u0161ibka burja. Najni\u017eje jutranje temperature bodo od 6 do 13, ob morju okoli 14, najvi\u0161je dnevne od 20 do 24, na Primorskem do 26 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Danes bo spremenljivo obla\u010dno, ve\u010d jasnine bo ob severnem Jadranu. Predvsem popoldne bodo nastale posamezne plohe ali nevihte.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Jutri ve\u010dinoma son\u010dno in nekoliko topleje.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad srednjo Evropo, Alpami in severnim Balkanom se \u0161e zadr\u017euje vi\u0161insko jedro hladnega in vla\u017enega zraka, ki nam prina\u0161a nestanovitno vreme.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V \u010detrtek in petek bo pove\u010dini jasno.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Danes bodo \u0161e nastajale krajevne plohe, jutri pa bo ve\u010d sonce in suho.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
        "tsUpdated": "2021-08-31T03:59:37+00:00", 
        "type": "FeatureCollection"
    }, 
