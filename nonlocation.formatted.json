    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Petek, 11.6.2021 ob 11h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-06-11 10:58"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": [
                    "Popoldne bo spremenljivo obla\u010dno s krajevnimi plohami ali nevihtami. Najvi\u0161je dnevne temperature bodo od 22 do 27 \u00b0C.\n", 
                    "Pono\u010di se bo delno zjasnilo. Jutranje temperature bodo od 9 do 16 \u00b0C."
                ], 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Jutri bo ve\u010d son\u010dnega vremena, popoldne bodo nastale le posamezne plohe ali nevihte. Najvi\u0161je dnevne temperature bodo od 25 do 29 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Popoldne bo spremenljivo obla\u010dno, nastajale bodo krajevne plohe in nevihte. Manj verjetne bodo ob morju.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Jutri bo ve\u010dinoma son\u010dno, popoldne bodo nastale le posamezne plohe ali nevihte", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad zahodno in srednjo Evropo je obmo\u010dje visokega zra\u010dnega tlaka. Od severa priteka k nam v vi\u0161inah razmeroma hladen in vla\u017een zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "Jutri Nad zahodno in srednjo Evropo je obmo\u010dje visokega zra\u010dnega tlaka. Od severa priteka k nam v vi\u0161inah postopno bolj suh zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V nedeljo bo ve\u010dinoma son\u010dno, popoldanske plohe bodo manj verjetne. V ponedeljek bo prete\u017eno jasno.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Popoldne bo spremenljivo obla\u010dno s krajevnimi plohami ali nevihtami. Jutri bo ve\u010d sonca, padavine bodo manj verjetne.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
