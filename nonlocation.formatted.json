    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Petek, 3.9.2021 ob 6h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-09-03 05:50"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Danes bo precej jasno. Najvi\u0161je dnevne temperature bodo od 22 do 27 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Jutri bo delno jasno, predvsem v ju\u017eni in zahodni Sloveniji ob\u010dasno zmerno obla\u010dno. Ponekod bo pihal veter vzhodnih smeri. Najni\u017eje jutranje temperature bodo od 8 do 12, na Primorskem do 15, najvi\u0161je dnevne od 22 do 27 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Danes bo son\u010dno.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Jutri bo na zahodnem Avstrijskem Koro\u0161kem ter na obmo\u010dju Karnijskih Alp spremenljivo obla\u010dno, nastalo bo nekaj ploh ali neviht. V krajih ju\u017eno od nas bo zmerno do prete\u017eno obla\u010dno, v krajih vzhodno od nas pa ve\u010dinoma son\u010dno.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad Balkanom, srednjo in severozahodno Evropo je obse\u017eno obmo\u010dje visokega zra\u010dnega tlaka. S \u0161ibkim severozahodnikom v vi\u0161inah k nam doteka dokaj suh zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V nedeljo in ponedeljek bo ve\u010dinoma son\u010dno.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Ve\u010dinoma bo son\u010dno, jutri bo v ju\u017eni in zahodni Sloveniji ob\u010dasno zmerno obla\u010dno.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
