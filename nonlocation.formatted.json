    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Torek, 1.3.2022 ob 6h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2022-03-01 05:51"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Danes bo precej jasno, nekaj ve\u010d obla\u010dnosti bo \u010dez dan na jugovzhodu. Pihal bo severovzhodnik, na Primorskem \u0161ibka do zmerna burja. Veter bo popoldne slabel. Najvi\u0161je dnevne temperature bodo od 3 do 7, na Primorskem do 12 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Jutri bo prete\u017eno jasno. Na Primorskem bo popoldne zapihal jugozahodnik. Najni\u017eje jutranje temperature bodo od -7 do -1, v alpskih dolinah do -11, najvi\u0161je dnevne od 7 do 12 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": [
                    "Danes bo v sosednjih pokrajinah Hrva\u0161ke zmerno do prete\u017eno obla\u010dno. Predvsem na obmo\u010dju Gorskega Kotarja bodo dopoldne \u0161e manj\u0161e padavine. Popoldne se bo postopno jasnilo. Drugod bo precej jasno. Veter bo popoldne slabel."
                ], 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Jutri bo prete\u017eno jasno, le zjutraj in deloma dopoldne bo v notranjosti Hrva\u0161ke nekaj obla\u010dnosti.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Na\u0161i kraji so na ju\u017enem obrobju obse\u017enega obmo\u010dja visokega zra\u010dnega tlaka. Od severovzhoda doteka k nam boj suh in razmeroma hladen zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": [
                    "V \u010detrtek bo \u0161e precej jasno vreme.", 
                    "V no\u010di na petek se bo poobla\u010dilo.", 
                    "V petek bo obla\u010dno z ob\u010dasnimi rahlimi padavinami."
                ], 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Danes bo son\u010dno, severovzhodni veter bo popoldne oslabel. Jutri bo prete\u017eno jasno.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
