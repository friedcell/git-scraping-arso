    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "\u010cetrtek, 27.1.2022 ob 6h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2022-01-27 05:43"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": [
                    "Danes bo zmerno do prete\u017eno obla\u010dno. Popoldne bo ponekod prehodno zapihal jugozahodni veter. Najvi\u0161je dnevne temperature bodo od 5 do 10 \u00b0C."
                ], 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Jutri bo delno jasno s spremenljivo obla\u010dnostjo. Zjutraj bo po nekaterih ni\u017einah megla. Najni\u017eje jutranje temperature bodo od -7 do -2, ob morju 0, najvi\u0161je dnevne od 4 do 8, na Gori\u0161kem do 10 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Danes bo zmerno do prete\u017eno obla\u010dno.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Jutri bo na nebu spremenljiva obla\u010dnost, po nekaterih ni\u017einah pa bo dopoldne megleno.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad zahodno Evropo, Alpami in severnim Sredozemljem je obmo\u010dje visokega zra\u010dnega tlaka. Od severozahoda doteka k nam v vi\u0161inah nekoliko toplej\u0161i in dokaj suh zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V soboto bo zmerno do prete\u017eno obla\u010dno. V nedeljo bo sprva \u0161e precej obla\u010dno ali megleno, \u010dez dan pa se bo razjasnilo.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Danes bo zmerno do prete\u017eno obla\u010dno. Jutri bo delno jasno s spremenljivo obla\u010dnostjo.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
