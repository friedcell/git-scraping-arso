    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Petek, 23.7.2021 ob 18h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-07-23 17:33"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Pono\u010di se bo postopno povsod razjasnilo. Najni\u017eje jutranje temperature bodo od 13 do 17, ob morju okoli 20 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "V soboto bo son\u010dno, popoldne bo ponekod zapihal jugozahodni veter. Najvi\u0161je dnevne temperature bodo od 27 do 31 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Zve\u010der bodo plohe in nevihte povsod ponehale, pono\u010di se bo od zahoda postopno zjasnilo.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "V soboto bo prevladovalo son\u010dno poletno vreme.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Prek vzhodnih Alp se proti jugovzhodu pomika vi\u0161insko jedro hladnega zraka. Njegov vpliv na vreme pri nas bo zve\u010der hitro oslabel.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V soboto Nad jugovzhodno Evropo je \u0161ibko obmo\u010dje visokega zra\u010dnega tlaka. Od jugozahoda doteka k nam razmeroma topel in suh zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": [
                    "V nedeljo in ponedeljek bo ve\u010dinoma son\u010dno in vro\u010de. Verjetnost neviht bo majhna in omejena predvsem na gorati svet severozahodne Slovenije. Pihal bo jugozahodni veter, ob morju jugo.", 
                    "V torek bo ve\u010d obla\u010dnosti, predvsem v zahodni in osrednji Sloveniji bodo nastajale plohe in nevihte."
                ], 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Pono\u010di se bo postopno povsod razjasnilo. Jutri bo son\u010dno in vro\u010de.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
        "tsUpdated": "2021-07-23T15:33:19+00:00", 
        "type": "FeatureCollection"
    }, 
