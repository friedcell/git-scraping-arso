    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Sobota, 20.3.2021 ob 6h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-03-20 05:52"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Danes bo na Primorskem zmerno, drugod spremenljivo do prete\u017eno obla\u010dno. Pojavljale se bodo sne\u017ene plohe. Na Primorskem bo pihala \u0161ibka do zmerna burja, drugod severovzhodni veter. Najvi\u0161je dnevne temperature bodo od 1 do 6, na Gori\u0161kem in ob morju okoli 10 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Jutri bo pove\u010dini son\u010dno. \u0160e bo pihal severovzhodni veter, na Primorskem \u0161ibka do zmerna burja. Najni\u017eje jutranje temperature bodo od -7 do -3, v alpskih dolinah do -10, na Primorskem okoli 0, najvi\u0161je dnevne od 3 do 8, na Primorskem do 12 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Danes bo ob severnem Jadranu dokaj son\u010dno, krepila se bo burja. Drugod bo spremenljivo do prete\u017eno obla\u010dno s sne\u017enimi plohami. Pihal bo severovzhodni veter.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Jutri bo ve\u010dinoma son\u010dno z ob\u010dasno zmerno obla\u010dnostjo. Pihal bo severovzhodni veter, ob severnem Jadranu burja.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad zahodno in srednjo Evropo ter Alpami je obmo\u010dje visokega zra\u010dnega tlaka. Od severa doteka k nam hladen in ob\u010dasno bolj vla\u017een zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V ponedeljek in torek bo deloma son\u010dno, a hladno in vetrovno. Pihal bo okrepljen severni do severovzhodni veter, na Primorskem zmerna burja.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Danes bo na Primorskem ob burji zmerno, drugod prete\u017eno obla\u010dno s sne\u017enimi plohami. Jutri bo ve\u010dinoma son\u010dno in vetrovno.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
