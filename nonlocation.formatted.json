    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "Ponedeljek, 14.6.2021 ob 18h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-06-14 17:36"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Pono\u010di bo prete\u017eno jasno. Najni\u017eje jutranje temperature bodo od 7 do 12, na Primorskem do 15 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "V torek bo ve\u010dinoma jasno. Najvi\u0161je dnevne temperature bodo od 25 do 28 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Pono\u010di bo ve\u010dinoma jasno. \u0160ibka burja v Kvarnerju bo ponehala.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "V torek bo v vseh sosednjih pokrajinah prete\u017eno jasno.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad zahodno in srednjo Evropo se krepi obmo\u010dje visokega zra\u010dnega tlaka. S severnimi vetrovi k nam priteka suh in postopno toplej\u0161i zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V torek Nad zahodno in srednjo Evropo se krepi obmo\u010dje visokega zra\u010dnega tlaka. S severnimi vetrovi k nam priteka suh in postopno toplej\u0161i zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "V sredo, \u010detrtek in petek bo son\u010dno in vsak dan nekoliko topleje. Verjetnost za popoldanske nevihte bo precej majhna.", 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Pono\u010di in jutri bo prete\u017eno jasno.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
