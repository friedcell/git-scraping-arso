    "fcast_si_text": {
        "articleinfo": {
            "pubdate": "\u010cetrtek, 14.1.2021 ob 16h", 
            "title": "Vremenska napoved", 
            "tsValid_issued": "2021-01-14 16:08"
        }, 
        "dataType": "", 
        "features": "", 
        "params": "", 
        "section": [
            {
                "para": "Pono\u010di bo prete\u017eno jasno, ponekod bo \u0161e pihal severni veter. Najni\u017eje jutranje temperature bodo od -13 do -6, ob morju okoli 0 \u00b0C.", 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": [
                    "Jutri bo prete\u017eno jasno, na Primorskem bo pihala ve\u010dinoma \u0161ibka burja.", 
                    "Najvi\u0161je dnevne temperature bodo od -2 do 2, na Primorskem do 8 \u00b0C."
                ], 
                "title": "NAPOVED ZA SLOVENIJO"
            }, 
            {
                "para": "Pono\u010di se bo postopno zjasnil. \u0160e bo vetrovno, v Alpah bo pihal mo\u010dan severnik, v Kvarnerju zmerna burja.", 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": [
                    "Jutri bo prete\u017eno jasno, v Kvarnerju bo pihala zmerna burja."
                ], 
                "title": "NAPOVED ZA SOSEDNJE POKRAJINE"
            }, 
            {
                "para": "Nad srednjo Evropo se krepi obmo\u010dje visokega zra\u010dnega tlaka. S severnimi vetrovi k nam priteka dokaj hladen in postopoma bolj suh zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": "Jutri Nad zahodno in srednjo Evropo se krepi obmo\u010dje visokega zra\u010dnega tlaka. S severovzhodnikom k nam priteka precej hladen in suh zrak.", 
                "title": "VREMENSKA SLIKA"
            }, 
            {
                "para": [
                    "V soboto bo na zahodu prete\u017eno jasno, drugod bo delno jasno z zmerno obla\u010dnostjo, na vzhodu tudi prete\u017eno obla\u010dno. ", 
                    "V nedeljo se bo poobla\u010dilo na zahodu, v osrednjih in vzhodnih krajih pa bo deloma son\u010dno.  "
                ], 
                "title": "OBETI"
            }, 
            {
                "para": "Dodatnega opozorila ni.", 
                "title": "OPOZORILO"
            }, 
            {
                "para": "Pono\u010di bo prete\u017eno jasno, ponekod bo \u0161e pihal severni veter. Zjutraj bo precej hladno. Jutri bo prete\u017eno jasno.", 
                "title": "NAPOVED ZA SLOVENIJO - povzetek"
            }
        ], 
