ost"},"wwsyn_icon":{"name":"PHENOM.","unit":"","desc":"Pojavi"},"clouds_icon_wwsyn_icon":{"name":"NPHENOM.","unit":"","desc":"Oblačnost / Pojavi"},"wwsyn_shortText":{"name":"PHENOM.","unit":"","desc":"Pojavi"},"wwsyn_decodeText":{"name":"PHENOM.","unit":"","desc":"Pojavi"},"msl":{"name":"MSL","unit":"hPa","desc":"Tlak"},"t":{"name":"T","unit":"°C","desc":"Temperatura"},"sn_acc":{"name":"SN","unit":"mm","desc":"Snežne padavine"},"rh":{"name":"RH","unit":"%","desc":"Vlažnost"},"ddff_icon":{"name":"DD/FF","unit":"","desc":"Veter"},"ff_val":{"name":"FF","unit":"km/h","desc":"Hitrost vetra"},"cloudBase_shortText":{"name":"hhnh","unit":"","desc":"Višina spodnjega sloja oblačnosti"}},"type":"FeatureCollection"}}