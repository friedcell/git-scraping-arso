nost"},"wwsyn_shortText":{"name":"PHENOM.","unit":"","desc":"Pojavi"},"msl":{"name":"MSL","unit":"hPa","desc":"Tlak"},"t":{"name":"T","unit":"°C","desc":"Temperatura"},"sn_acc":{"name":"SN","unit":"mm","desc":"Snežne padavine"},"tp_acc":{"name":"TP","unit":"mm","desc":"Padavine"},"ddff_icon":{"name":"DD/FF","unit":"","desc":"Veter"},"ff_val":{"name":"FF","unit":"km/h","desc":"Hitrost vetra"},"cloudBase_shortText":{"name":"hhnh","unit":"","desc":"Višina spodnjega sloja oblačnosti"}},"type":"FeatureCollection"}}